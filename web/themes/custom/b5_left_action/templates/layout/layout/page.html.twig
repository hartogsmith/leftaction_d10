{#
/**
 * @file
 * Default theme implementation to display a single interior page page.
 * CUSTOM
#}

{% if page.tracking_header %}
	{{ page.tracking_header }}
{% endif %}

{# Navbar #}
<nav class="navbar navbar-main navbar-dark bg-t fixed-top" aria-label="Branding and navigation">
	<div class="container">

		{{ page.navigation_left }}
		<button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#mainNavBar" aria-controls="mainNavBar" aria-expanded="false" aria-label="Toggle navigation">
			<i class="fa-solid fa-bars"></i>
		</button>

		<div class="navbar-collapse collapse" id="mainNavBar" style="">
			{{ page.navigation_collapsible }}
			{{ page.navigation_collapsible_right }}
		</div>

	</div>
</nav>

<div class="page-container">

	{# Main #}

		{% if page.home_jumbotron %}
			{{ page.home_jumbotron }}
		{% endif %}

		{% if is_front %}


			{% if page.home_block_1 or page.home_block_2 or page.home_block_3 %}
			{% set home_blocks = 0 %}
			{% if page.home_block_1 %}
				{% set home_blocks = home_blocks + 1 %}
			{% endif %}
			{% if page.home_block_2 %}
				{% set home_blocks = home_blocks + 1 %}
			{% endif %}
			{% if page.home_block_3 %}
				{% set home_blocks = home_blocks + 1 %}
			{% endif %}
			{% set home_blocks = 12 / home_blocks %}
			<!--<h1>home_blocks = {{ home_blocks }}</h1>-->
			<div class="container">
				<div class="row my-5 home-{{ home_blocks }}">
					{% if page.home_block_1 %}
						<section class="home-block-1 col-md-{{ home_blocks }}">
						<div class="inner">
						{{ page.home_block_1 }}
						</div>
						</section>
					{% endif %}

					{% if page.home_block_2 %}
						<section class="home-block-2 col-md-{{ home_blocks }}">
						<div class="inner">
						{{ page.home_block_2 }}
						</div>
						</section>
					{% endif %}	

					{% if page.home_block_3 %}
						<section class="home-block-3 col-md-{{ home_blocks }}">
						<div class="inner">
						{{ page.home_block_3 }} 
						</div>
						</section>
					{% endif %}
				</div>
			</div>
			{% endif %}

			{% else %}
			<div id="main_content" role="main" class="main-container-outer js-quickedit-main-content">


				<div class="main-container container py-5">
					{# Interior #}
					<section>

						{# Highlighted #}
						{% if page.highlighted %}
						<div class="highlighted">{{ page.highlighted }}</div>
						
						{% endif %}

						{# Breadcrumbs #}
						{% if breadcrumb %}
						{{ breadcrumb }}
						
						{% endif %}

						{# Action Links #}
						{% if action_links %}
						<ul class="action-links">{{ action_links }}</ul>
						{% endif %}

						{# Help #}
						{% if page.help %}
						{{ page.help }}       
						{% endif %}

						{# Non-home Content #}
						{{ page.content }}

					</section>
					
				</div>
			</div><!-- /main-container -->
		{% endif %}

	{% if page.after_content %}
		  <div class="after-content">
  			{{ page.after_content }}
      	  </div>
	{% endif %}	

	{% if page.footer_action %}
		  <div class="footer-action text-inverse">
  			{{ page.footer_action }}
          </div>
	{% endif %}	

	{% if (page.footer_top_center) or (page.footer_upper_left) or (page.footer_upper_right) or (page.footer_lower_left) or (page.footer_lower_right) or (page.footer_bottom_center) %}
		<footer class="main-footer-outer text-inverse">
			<div class="main-footer container">
				
					{% if page.footer_top_center %}
						<div class="footer-top-outer row">
							<div class="footer-top-center">{{ page.footer_top_center }}</div>
						</div>
					{% endif %}
				

					{% if page.footer_upper_left or page.footer_upper_right %}
						<div class="footer-upper-outer row">
							{% if page.footer_upper_left %}
									<div class="footer-upper-left col-sm-6">{{ page.footer_upper_left }}</div>	
							{% endif %}
							
							{% if page.footer_upper_right %}
								<div class="footer-upper-right col-sm-6">{{ page.footer_upper_right }}</div>
							{% endif %}
						</div>
					{% endif %}

					{% if page.footer_center_center %}
						<div class="footer-center-outer row">
							{% if page.footer_center_center %}
								<div class="footer-center-center col col-sm-12">{{ page.footer_center_center }}</div>
							{% endif %}
						</div>
					{% endif %}

					{% if page.footer_lower_left or page.footer_lower_right %}
						<div class="footer-lower-outer row">			
							{% if page.footer_lower_left %}
								<div class="footer-lower-left col-sm-6">{{ page.footer_lower_left }}</div>
							{% endif %}
							{% if page.footer_lower_right %}
								<div class="footer-lower-right col-sm-6">{{ page.footer_lower_right }}</div>
							{% endif %}	
						</div>
					{% endif %}

					{% if page.footer_bottom_center %}
						<div class="footer-bottom-outer row">
							<div class="footer-bottom-center">{{ page.footer_bottom_center }}</div>
						</div>
					{% endif %}

					{% if page.disclaimer %}
						<div class="disclaimer-outer row">
							<div class="disclaimer">
								{{ page.disclaimer }}
							</div>
						</div>
					{% endif %}

			</div>
		</footer>
	{% endif %}

</div><!-- /page-container -->

{% if page.tracking_footer %}
	{{ page.tracking_footer }}
{% endif %}